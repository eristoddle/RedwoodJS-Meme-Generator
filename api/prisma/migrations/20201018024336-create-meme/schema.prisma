datasource DS {
  // optionally set multiple providers
  // example: provider = ["sqlite", "postgresql"]
  provider = "postgresql"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Meme {
  id        Int      @id @default(autoincrement())
  userId    String
  image     String
  createdAt DateTime @default(now())
}
